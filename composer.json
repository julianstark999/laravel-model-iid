{
    "name": "julianstark999/laravel-model-iid",
    "description": "Add an internal ID to your Eloquent Model",
    "keywords": [
        "Julian Stark",
        "julianstark999",
        "laravel",
        "laravel-model-iid",
        "iid",
        "internal id",
        "additional id"
    ],
    "homepage": "https://github.com/julianstark999/laravel-model-iid",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Julian Stark",
            "email": "julianstark1@googlemail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/console": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-package-tools": "^1.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^7.0",
        "vimeo/psalm": "^4.6"
    },
    "autoload": {
        "psr-4": {
            "JulianStark999\\LaravelModelIid\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "JulianStark999\\LaravelModelIid\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "lint": "php-cs-fixer fix -v",
        "psalm": "psalm"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "JulianStark999\\LaravelModelIid\\LaravelModelIidServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
